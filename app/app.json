{
  "expo": {
    "name": "app",
    "slug": "app",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./res/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./res/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./res/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jorensm.app",
      "versionCode": 2
    },
    "web": {
      "favicon": "./res/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e517c0ae-a3f2-469b-91e1-d264be03a5ba"
      }
    }
  }
}
